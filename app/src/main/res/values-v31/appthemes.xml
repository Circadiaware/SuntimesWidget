<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <string-array name="appThemes_display" tools:ignore="InconsistentArrays"> <!-- display values (one-to-one with appThemes_values) -->
        <item>@string/appThemes_lightTheme</item>
        <item>@string/appThemes_darkTheme</item>
        <item>@string/appThemes_nightMode</item>
        <item>@string/appThemes_systemDefault</item>
    </string-array>
    <string-array name="appThemes_values" tools:ignore="InconsistentArrays" translatable="false">  <!-- config values (one-to-one with appThemes_display) -->
        <item>light</item>
        <item>dark</item>
        <item>daynight</item>
        <item>system</item>
    </string-array>

    <string-array name="appThemes_light_display" tools:ignore="InconsistentArrays"> <!-- display values (one-to-one with appThemes_dark_values) -->
        <item>@string/configLabel_tagDefault</item>
        <item>@string/appThemes_systemDefault1</item>
        <item>@string/appThemes_lightTheme1</item>
        <item>@string/appThemes_monet</item>
    </string-array>
    <string-array name="appThemes_light_values" translatable="false" tools:ignore="InconsistentArrays">  <!-- config values (one-to-one with appThemes_dark_display) -->
        <item>default</item>
        <item>contrast_system</item>
        <item>contrast_light</item>
        <item>myou</item>
    </string-array>

    <string-array name="appThemes_dark_display" tools:ignore="InconsistentArrays"> <!-- display values (one-to-one with appThemes_dark_values) -->
        <item>@string/configLabel_tagDefault</item>
        <item>@string/appThemes_systemDefault1</item>
        <item>@string/appThemes_darkTheme1</item>
        <item>@string/appThemes_monet</item>
    </string-array>
    <string-array name="appThemes_dark_values" translatable="false" tools:ignore="InconsistentArrays">  <!-- config values (one-to-one with appThemes_dark_display) -->
        <item>default</item>
        <item>contrast_system</item>
        <item>contrast_dark</item>
        <item>myou</item>
    </string-array>


</resources>